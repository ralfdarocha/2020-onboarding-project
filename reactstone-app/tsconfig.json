{
    "compilerOptions": {
		"outDir": "./dist/",
		"noImplicitAny": true,
		"target": "es5",
        "lib": ["es5", "es6", "dom"],
        "typeRoots": ["./node_modules/@types", "./src/typings"],
		"jsx": "react",
		"allowJs": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "include": ["src/**/*"],
}
  